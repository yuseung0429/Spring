<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:p="http://www.springframework.org/schema/p"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<bean id="exam" class="spring.di.entity.Exam2023">
	
	<!-- Construction injection 2-->
	<!-- <bean id="exam" class="spring.di.entity.Exam2023" p:kor="20" p:eng="40" p:math="60"> -->
		
		<!-- Setter injection -->
<!-- 	<property name="kor" value="20"></property>
		<property name="eng" value="40"></property>
		<property name="math" value="60"></property>
		<property name="com" value="80"></property> -->
		
		<!-- Construction injection 1-->
		<constructor-arg index = "0" value="20"></constructor-arg>
		<constructor-arg index = "1" value="40"></constructor-arg>
		<constructor-arg index = "3" value="80"></constructor-arg>
		<constructor-arg index = "2" value="60"></constructor-arg>
		</bean>
	<bean id="console" class="spring.di.ui.GridExamConsole">
		<property name="exam" ref="exam"></property>
	</bean>
</beans>